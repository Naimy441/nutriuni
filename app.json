{
  "expo": {
    "name": "nutriuni",
    "slug": "nutriuni",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nutriuni.png",
    "scheme": "nutriuni",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Track your nutrition at Duke University dining locations",
    "keywords": [
      "nutrition",
      "duke",
      "university",
      "dining",
      "health",
      "tracker"
    ],
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nutriuni.app",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDisplayName": "nutriuni",
        "NSCameraUsageDescription": "This app does not use the camera.",
        "NSMicrophoneUsageDescription": "This app does not use the microphone.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.nutriuni.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/nutriuni.png",
        "backgroundColor": "#006838"
      },
      "edgeToEdgeEnabled": true,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/nutriuni.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/nutriuni.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#006838"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5355a882-2d82-41fe-baea-4d03383e5a28"
      }
    }
  }
}
